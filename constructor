// creating a function

function Student(name, age, language) 
{
    this.name = name;
    this.age = age;
    this.language = language;
}

// loading the vcalues

var student1 = new Student("Name", 12, "Bangla");
var student2 = new Student("Name2", 69, "English");

// displaying the value

console.log(student1.language);
console.log(student2.age);
